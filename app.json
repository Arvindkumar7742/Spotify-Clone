{
  "expo": {
    "name": "myspotifyapp",
    "slug": "myspotifyapp",
    "scheme": "myspotifyapp",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.webp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon-spotify.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.myspotifyapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.webp",
        "backgroundColor": "#ffffff"
      },
      "usesCleartextTraffic": true,
      "package": "com.anonymous.myspotifyapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "mini-page-builder-smoky.vercel.app",
              "pathPrefix": "/app"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./app",
          "asyncRoutes": true
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false,
        "root": "./app",
        "asyncRoutes": true
      },
      "eas": {
        "projectId": "b93814db-6fd4-4d6c-97f6-87b84076218d"
      }
    }
  }
}
