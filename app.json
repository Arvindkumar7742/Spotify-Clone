{
  "expo": {
    "name": "myspotifyapp",
    "slug": "myspotifyapp",
    "scheme": "myspotifyapp",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.myspotifyapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "usesCleartextTraffic": true,
      "package": "com.anonymous.myspotifyapp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./app",
          "asyncRoutes": true
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false,
        "root": "./app",
        "asyncRoutes": true
      },
      "eas": {
        "projectId": "b93814db-6fd4-4d6c-97f6-87b84076218d"
      }
    }
  }
}
